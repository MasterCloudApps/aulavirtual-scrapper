{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": "off",
        "import/prefer-default-export": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off"
    }
}
